package day0405.ioEx01;

import java.io.ByteArrayOutputStream;

public class ByteArrayOutputStreamEx01 {
	public static void main(String[] args) {
		byte [] result;
		
		//바이트 아웃풋 스트림 객체 생성
		ByteArrayOutputStream out=new ByteArrayOutputStream();
		
		out.write(0);
		
		out.write(1);
		
		out.write(2);
		
		out.write(3);
		
		out.write(4);
	}
}
