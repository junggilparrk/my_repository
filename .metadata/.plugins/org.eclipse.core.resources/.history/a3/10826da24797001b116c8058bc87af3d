package day0407Ex;

import java.io.File;
import java.io.IOException;

public class Test {
	public static void main(String[] args) {
		File file=new File("2021/04/07/testMyfile");
		if(file.exists()) {
			System.out.println("있다.");
		}else {
			System.out.println("없다");
		}
		if(file.isDirectory()) {
			System.out.println("있다");
		}else {
			System.out.println("없다");
		}
	
	}
}
 