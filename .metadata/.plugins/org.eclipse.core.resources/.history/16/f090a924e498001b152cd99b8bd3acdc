package day0409Ex;

public class MyTcpClient {
	public static void main(String[] args) {
		Thread t=new ClientThreadReader();
		Thread t1=new ClientThreadWriter();
		ClientThreadWriter t2= new ClientThreadWriter(); 
		t2.getSocket("127.0.0.1",7777);		
		while(true) {
			t.start();
			t1.start();
		}
	}
}
