package day0409Ex;

import java.io.BufferedReader;
import java.io.IOException;

public class SeverRead extends Thread{
	BufferedReader br;
	
		
	
	
	
	public SeverRead(BufferedReader br) {
		this.br = br;
	}

	@Override
	public void run() {
		String msg;
	try {	while(true) {
			msg = br.readLine();
			System.out.println(msg);
	}
		} catch (IOException e) {
			e.printStackTrace();
		}
				
		}
	}


