package day0409Ex;

import java.io.BufferedReader;
import java.util.Scanner;

public class ThreadEx2 extends Thread{
	BufferedReader br;
	
		
	
	
	public void buffer(BufferedReader br) {
		this.br = br;
	}

	@Override
	public void run() {
		Scanner sc=new Scanner(System.in);
		while(true) {
			String str=sc.nextLine();
				
		}
	}

}
