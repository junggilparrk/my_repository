package day0407Homework;

import java.io.BufferedOutputStream;
import java.io.File;
import java.io.FileNotFoundException;
import java.io.FileOutputStream;
import java.io.IOException;

public class MyGugu {
	public static void main(String[] args) {
		File file=new File("src/day0407Homework/makeGuguFile()/gugu.txt");
		if(file.isDirectory()) {
		}else {
			file.mkdirs();
			try {
				file.createNewFile();
			} catch (IOException e) {
				e.printStackTrace();
			}
		}
		System.out.println((int)'=');
		int k=2;
		try(BufferedOutputStream bout=new BufferedOutputStream(new FileOutputStream(file))){
			for(int i=1;i<10;i++) {
				bout.write(2);
				bout.write(42);
				bout.write(k);
				
			}
			
		} catch (FileNotFoundException e) {
			e.printStackTrace();
		} catch (IOException e) {
			e.printStackTrace();
		}
		
		
	}
}
